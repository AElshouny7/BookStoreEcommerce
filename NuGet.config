<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <!-- Stop inheriting machine/global sources (including Azure Artifacts) -->
        <clear />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    </packageSources>
    <!-- (Optional) Explicitly disable the Azure feed by name if it leaks in -->
    <disabledPackageSources>
        <add key="gameballers-stg" value="true" />
    </disabledPackageSources>
</configuration>
